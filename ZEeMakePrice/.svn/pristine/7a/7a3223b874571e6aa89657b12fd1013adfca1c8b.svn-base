package ezo.mp.domain;

public class Order extends Users {
	private String ordernumber;
	private int totalamount;
	private String cartnumber;
	private String recontext;
	private String payment;
	private String productcode;
	private String productname;
	private String ea;

	public String getOrdernumber() {
		return ordernumber;

	}

	public void setOrdernumber(String ordernumber) {
		this.ordernumber = ordernumber;
	}

	public int getTotalamount() {
		return totalamount;
	}

	public void setTotalamount(int totalamount) {
		this.totalamount = totalamount;
	}

	public String getCartnumber() {
		return cartnumber;
	}

	public void setCartnumber(String cartnumber) {
		this.cartnumber = cartnumber;
	}

	public String getRecontext() {
		return recontext;
	}

	public void setRecontext(String recontext) {
		this.recontext = recontext;
	}

	public String getPayment() {
		return payment;
	}

	public void setPayment(String payment) {
		this.payment = payment;
	}

	public String getProductcode() {
		return productcode;
	}

	public void setProductcode(String productcode) {
		this.productcode = productcode;
	}

	public String getProductname() {
		return productname;
	}

	public void setProductname(String productname) {
		this.productname = productname;
	}

	public String getEa() {
		return ea;
	}

	public void setEa(String ea) {
		this.ea = ea;
	}

	@Override
	public String toString() {
	
		return 	super.toString()+"Order [ordernumber=" + ordernumber + ", totalamount="
				+ totalamount + ", cartnumber=" + cartnumber + ", recontext="
				+ recontext + ", payment=" + payment + ", productcode="
				+ productcode + ", productname=" + productname + ", ea=" + ea
				+ "]";
	}

}